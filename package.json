  "devDependencies": {
    "clean-css": "^5.2.4",
    "clean-css-cli": "^5.5.2",
    "concurrently": "^7.0.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "description": ""
}
